package myclient;

import java.net.ServerSocket;

public class ServerMain {
	public ServerMain() {
		System.out.println("서버 가동!!");
		
		ServerSocket sSoc = null;
		try {
			new ServerSocket(3000); 
		} catch(Exception e) {
			e.printStackTrace();
		} finally {
			try { 
				if( sSoc != null ) {
					sSoc.close();
				}
			}
		}
		
		
		System.out.println("서버 종료!!");
	}
	
	
	public static void main(String[] args) {
		new ServerMain();
	}
}
